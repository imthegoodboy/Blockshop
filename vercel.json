{
  "version": 2,
  "builds": [{ "src": "web/package.json", "use": "@vercel/next" }],
  "routes": [{ "src": "/(.*)", "dest": "/web/$1" }],
  "github": {
    "silent": true,
    "autoJobCancelation": true,
    "installCommand": "cd web && npm install",
    "buildCommand": "cd web && npm run build"
  }
}
